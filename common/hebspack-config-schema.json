          "title": "The Environment Schema",
          "default": "",
          "examples": [
            "production"
          ],
          "pattern": "^(.*)$"
        },
        "taskSeries": {
          "$id": "#/properties/generalOptions/properties/taskSeries",
          "type": "array",
          "title": "The Taskseries Schema",
          "items": {
            "$id": "#/properties/generalOptions/properties/taskSeries/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "slug",
              "run",
              "tasks"
            ],
            "properties": {
              "slug": {
                "$id": "#/properties/generalOptions/properties/taskSeries/items/properties/slug",
                "type": "string",
                "title": "The Slug Schema",
                "default": "",
                "examples": [
                  "init"
                ],
                "pattern": "^(.*)$"
              },
              "run": {
                "$id": "#/properties/generalOptions/properties/taskSeries/items/properties/run",
                "type": "boolean",
                "title": "The Run Schema",
                "default": false,
                "examples": [
                  true
                ]
              },
              "tasks": {
                "$id": "#/properties/generalOptions/properties/taskSeries/items/properties/tasks",
                "type": "array",
                "title": "The Tasks Schema",
                "items": {
                  "$id": "#/properties/generalOptions/properties/taskSeries/items/properties/tasks/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "style-init",
                    "script-init",
                    "image-init",
                    "font-init",
                    "php-plugins-init"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    },
